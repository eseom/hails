      "babel-plugin-syntax-export-extensions",
      "babel-plugin-transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eseom/hails"
  },
  "dependencies": {
    "babel-plugin-import": "^1.6.0",
    "babel-plugin-syntax-export-extensions": "^6.13.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "blipp": "^2.3.0",
    "catbox-disk": "^2.0.9",
    "catbox-redis": "^3.1.1",
    "chokidar": "^1.7.0",
    "hapi": "^16.6.2",
    "hapi-es7-async-handler": "^1.0.11",
    "hapi-swagger": "^7.8.1",
    "hoek": "^5.0.0",
    "hotload": "^0.0.5",
    "inert": "^4.2.1",
    "kue": "^0.11.6",
    "kue-scheduler": "^0.7.4",
    "mysql": "^2.15.0",
    "nes": "^6.4.3",
    "nunjucks": "^3.0.1",
    "pg": "^7.3.0",
    "pg-native": "^2.2.0",
    "sequelize": "^4.13.5",
    "sequelize-cli": "^3.0.0",
    "sqlite3": "^3.1.13",
    "vision": "^4.1.1",
    "winston": "^2.4.0",
    "yar": "^8.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0"

{
  "name": "hails",
  "version": "0.4.3",
  "main": "index.js",
  "license": "MIT",
  "bin": {
    "hails": "bin/hails"
  },
  "scripts": {
    "build": "babel src -d lib",
    "watch": "babel src -d lib --watch",
    "lint": "eslint src/*",
    "prepublish": "yarn build"
  },
  "babel": {
    "sourceMaps": "inline",
    "retainLines": true,
    "plugins": [
      "babel-plugin-transform-es2015-modules-commonjs",
      "babel-plugin-syntax-export-extensions",
      "babel-plugin-transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eseom/hails"
  },
  "dependencies": {
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-syntax-export-extensions": "^6.13.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "blipp": "^3.0.0",
    "catbox-redis": "^4.0.0",
    "chokidar": "^1.7.0",
    "hapi": "^17.2.0",
    "hapi-swagger": "^9.0.0",
    "hotload": "^0.0.5",
    "inert": "^5.0.1",
    "kue-scheduler": "^0.7.4",
    "mysql": "^2.15.0",
    "nes": "^7.0.2",
    "pg": "^7.4.0",
    "sequelize": "^4.28.6",
    "sequelize-cli": "^3.2.0",
    "sqlite3": "^3.1.13",
    "twig": "^1.10.5",
    "vision": "5.2.0",
    "winston": "^3.0.0-rc1",
    "yar": "^9.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0"
  }
}

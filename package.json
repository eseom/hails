{
  "name": "hails",
  "version": "0.3.7",
  "main": "index.js",
  "license": "MIT",
  "bin": {
    "hails": "bin/hails"
  },
  "scripts": {
    "build": "babel src -d lib",
    "watch": "babel src -d lib --watch",
    "lint": "eslint src/*",
    "prepublish": "yarn build"
